{
  "name": "userstyles",
  "description": "A collection of userstyles for simple and boring dark themes",
  "author": "Arun Chandanaveli <aruncveli@gmail.com> (https://github.com/aruncveli)",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "userstyles",
    "stylus",
    "dark-theme"
  ],
  "scripts": {
    "check": "biome check && prettier --check .",
    "fix": "biome check --write && prettier --write --log-level warn .",
    "fix:biome": "biome check --write",
    "generate": "bun scripts/generate.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tsconfig/strictest": "2.0.8",
    "@types/bun": "1.3.4",
    "@types/ejs": "3.1.5",
    "@types/node": "25.0.2",
    "ejs": "3.1.10",
    "lefthook": "2.0.11",
    "prettier": "3.7.4",
    "typescript": "5.9.3"
  }
}
