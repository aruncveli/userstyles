{
  "name": "userstyles",
  "description": "A collection of userstyles for simple and boring dark themes",
  "author": "Arun Chandanaveli <aruncveli@gmail.com> (https://github.com/aruncveli)",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "userstyles",
    "stylus",
    "dark-theme"
  ],
  "scripts": {
    "check": "biome check && prettier --check .",
    "fix": "biome check --write && prettier --write .",
    "fix:biome": "biome check --write",
    "generate": "bun scripts/generate.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/bun": "1.2.17",
    "@types/ejs": "3.1.5",
    "@types/node": "24.0.3",
    "ejs": "3.1.10",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  }
}
