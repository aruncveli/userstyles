{
	"name": "userstyles",
	"description": "A collection of userstyles for simple and boring dark themes",
	"author": "Arun Chandanaveli <aruncveli@gmail.com> (https://github.com/aruncveli)",
	"license": "MIT",
	"type": "module",
	"keywords": ["userstyles", "stylus", "dark-theme"],
	"scripts": {
		"check": "biome check && eslint . && prettier --check .",
		"fix": "biome check --write && eslint --fix . && prettier --write .",
		"fix:biome": "biome check --write",
		"generate": "bun templates/generate.ts",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@eslint/js": "9.21.0",
		"@tsconfig/strictest": "2.0.5",
		"@types/ejs": "3.1.5",
		"@types/node": "22.13.9",
		"ejs": "3.1.10",
		"eslint": "9.21.0",
		"eslint-config-prettier": "10.0.2",
		"husky": "9.1.7",
		"lint-staged": "15.4.3",
		"prettier": "3.5.3",
		"typescript": "5.8.2",
		"typescript-eslint": "8.26.0"
	}
}
